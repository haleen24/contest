#include <iostream>
#include <vector>
#include <algorithm>

int main()
{
	int n, a, b, c;
	
	std::cin >> n >> a >> b >> c;

	std::vector <int> arr = { a,b,c };

	sort(arr.begin(), arr.end());

	c = arr[0];

	b = arr[1];

	a = arr[2];

	int sum = 0;

	for (int i1 = n; i1 >= 0; i1--) {

		for (int i2 = 0; i2 < n; i2++) {

			for (int i3 = 0; i3 < n; i3++) {

				sum = i1 * c + i2 * b + i3 * a;

				if (sum == n) {

					std::cout << i1 + i2 + i3;

					return 0;
				}
			}

		}


	}
}
